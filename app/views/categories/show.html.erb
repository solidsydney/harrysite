<%= provide :title, "#{@category.name}"%>

<h3><%= @category.name %></h3>
<% @posts.in_groups_of(3, false) do |row_post|%>
<div class="row-fluid">
  <% row_post.each do |post| %>
    <%= render "post", post: post%>
  <% end %>
</div>
<% end %>
<div class="pagination">
  <ul>
    <%= will_paginate(@posts)%>
  </ul>
</div>

<!--END CONTENT-->