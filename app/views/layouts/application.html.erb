</head>
<body>
<!--HEADER-->
<%= render "header" %>

<% unless current_user %>
    <%= render "session_form"%>
<% end %>
<!--CONTAINER-->
<div class="container">
<!--MENU-->
  <div class="row-fluid">
    <%= render "category_menu"%>
  </div>

<!--END MENU-->

<!--MAIN SECTION-->
<div class="main post-page">
<div class="row-fluid" <% unless mobile_device? %> style="margin-bottom: 0" <% end %>>
<!--CONTENT-->
<div class="posts span9">
<%= yield %>
</div>
<!--END CONTENT-->

<!--SIDEBAR-->
<%= render "sidebar"%>
<!--END SIDEBAR-->
</div>

<!--POPULAR SECTION-->
<% if content_for?(:popular) %>

<%= yield(:popular) %>
<% end %>
<!--END POPULAR SECTION-->

<% if content_for?(:bottom_items) %>
  <%= yield(:bottom_items) %>
<% end %>

</div>
<!--END MAIN SECTION-->
<!--FOOTER-->
<footer>
<div class="rights clearfix">
    <p style="text-align: center; color: #fff">Â© <%= Time.now.year %> BlanckDigital All rights reserved. <%= link_to "Contact Us", contact_us_url %></p>
</div>
</footer>
<!--END FOOTER-->
</div>
<!--END CONTAINER-->
