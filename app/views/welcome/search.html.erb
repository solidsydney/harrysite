<h3>Search result: <span><%= @q %></span></h3>
<% @results.in_groups_of(3, false) do |row_post|%>
    <div class="row-fluid">
      <% row_post.each do |post| %>
          <%= render "categories/post", post: post%>
      <% end %>
    </div>
<% end %>
<div class="pagination">
  <ul>
    <%= will_paginate(@results)%>
  </ul>
</div>

